%html
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %title NmrWeb
    = stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => false
    = csrf_meta_tags
    / Basic Page Needs
    %meta{:charset => "utf-8"}/
    %title PLANAR | Personal trainer site template
    %meta{:content => "Only 16$ - PLANAR Personal trainer site template with calories calculators", :name => "description"}/
    %meta{:content => "Ansonika", :name => "author"}/
    / Favicons
    %link{:href => asset_path("favicon.ico"), :rel => "shortcut icon", :type => "image/x-icon"}/
    %link{:href => asset_path("apple-touch-icon-57x57-precomposed.png"), :rel => "apple-touch-icon", :type => "image/x-icon"}/
    %link{:href => asset_path("apple-touch-icon-72x72-precomposed.png"), :rel => "apple-touch-icon", :sizes => "72x72", :type => "image/x-icon"}/
    %link{:href => asset_path("apple-touch-icon-114x114-precomposed.png"), :rel => "apple-touch-icon", :sizes => "114x114", :type => "image/x-icon"}/
    %link{:href => asset_path("apple-touch-icon-144x144-precomposed.png"), :rel => "apple-touch-icon", :sizes => "144x144", :type => "image/x-icon"}/
    / Mobile Specific Metas
    %meta{:content => "width=device-width, initial-scale=1.0", :name => "viewport"}/
    %link{:href => "http://fonts.googleapis.com/css?family=Open+Sans:400,600,700,800,300", :rel => "stylesheet", :type => "text/css"}/
    /[if lt IE 9]
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    /[if IE 7]
      <link rel="stylesheet" href="font-awesome/css/font-awesome-ie7.min.css">
  %body
    %header
      .container
        .row
          .col-md-4.col-xs-3
            %h1#logo
              %a{:href => "/", :title => "Planar - Personal Trainer"} Planar - Personal Trainer
          %nav.col-md-8.col-xs-9
            %ul#main-nav.sf-menu
              %li
                %a{:href => "profile.html"} Profile
              %li
                %a{:href => "#"} Training
                %ul
                  %li
                    %a{:href => "services.html"} Services
                  %li
                    %a{:href => "training-programs.html"} Programs
              %li
                %a{:href => "#"} Calorie calculators
                %ul
                  %li
                    %a{:href => "daily-calorie-calculator.html"} Daily calories
                  %li
                    %a{:href => "calories-burned-heart-rate.html"} Calories burned by heart rate
                  %li
                    %a{:href => "calories-burned-calculator.html"} Calories burned by activites
                  %li
                    %a{:href => "bmi-calculator.html"} BMI calculator
              %li
                %a{:href => "#"} Pages
                %ul
                  %li
                    %a{:href => "pricing.html"} Pricing
                  %li
                    %a{:href => "blog.html"} Blog
                  %li
                    %a{:href => "blog_post.html"} Single post
                  %li
                    %a{:href => "elements.html"} Elements
                  %li
                    %a{:href => "img-galleries.html"} Images and galleries
                  %li
                    %a{:href => "media.html"} Media
              %li
                %a{:href => "/contact"} Contact
              %li
                %a{:href => "#"} Purchase
    .container
      = yield
    %footer
      .container
        .row
          .col-md-6
            %h5 Contacts
            %p
              Our mission is to bring affordable Technology education to people everywhere, in order to help them achieve their dreams and change the world. to bring affordable Technology education to people everywhere, in order to help them achieve their dreams and change the world.
            %ul
              %li
                %i.icon-user
                Personal trainer Name
              %li
                %i.icon-phone
                Telephone: 41.22.319.36.10
              %li
                %i.icon-envelope
                Email:
                %a{:href => "#"} info@planar.com
              %li
                %i.icon-skype
                Skype name: Planar
            %ul.social-bookmarks.clearfix
              %li.linkedin
                %a{:href => "#"} behance
              %li.facebook
                %a{:href => "#"} facebook
              %li.googleplus
                %a{:href => "#"} googleplus
              %li.twitter
                %a{:href => "#"} twitter
          .col-md-6
      #copy Â© 2013 - All Rhts Reserved
    / End footer
    #toTop Back to Top

    = javascript_include_tag 'application', 'data-turbolinks-track' => true
